{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<form id="payment" method="post" action="api/payment" enctype="application/x-www-form-urlencoded">
    <div class="container-fluid">
        <div class="form-group row">
            <label for="CID" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input type="text" id="CID" name="CID" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label for="v_cartid" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input type="text" id="v_cartid" name="v_cartid" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label for="v_currency" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input type="text" id="v_currency" name="v_currency" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label for="v_amount" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input type="text" id="v_amount" name="v_amount" class="form-control" />
            </div>
        </div>

        <input type="submit" value="Submit" class="btn btn-primary" />
    </div>
</form>
    


